#include <bits/stdc++.h>
#include <iostream>
using namespace std;

//(Note: Arrays are always passed by reference)

void print(int A[], int n){
    //Changing the 1st Value
    A[0] = 25;
    
    for(int i=0; i<n; i++){
        cout << A[i] << " ";
    }
}

int main() {

    int A[5] ={2,4,6,8,10};

    cout << "Before" << endl;
    for(int i=0; i<5; i++){
        cout << A[i] << " ";
    }

    cout << "\nAfter" << endl;
    print(A, 5);

return 0;
}